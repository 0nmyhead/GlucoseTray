<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>1.0.30</Version>
    <title>Dexcom.Fetch</title>
    <author>Chris Baker</author>
    <description>Use to easily get your latest real-time Glucose reading from Dexcom or your own Nightscout instance.</description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.0.0-preview6.19304.6" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
</Project>
